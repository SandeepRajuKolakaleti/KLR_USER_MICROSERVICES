    "@nestjs/config": "^2.3.0",
    "@nestjs/core": "^10.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^10.0.1",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/typeorm": "^9.0.0",
    "@types/multer": "^2.0.0",
    "aws-sdk": "^2.1413.0",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.2.4",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.3.1",
    "eslint-config-nodemailer": "^1.2.0",
    "eslint-config-prettier": "^9.1.0",
    "handlebars": "^4.7.8",
    "install": "^0.13.0",
    "mime-types": "^2.1.35",
    "mocha": "^10.2.0",
    "multer": "^2.0.2",
    "mysql2": "^3.6.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.9.9",
    "nodemailer-ntlm-auth": "^1.0.1",
    "npm": "^10.2.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
